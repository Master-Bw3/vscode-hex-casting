{
    "Considered Intro/Retro": {
        "prefix": "\\{}",
        "body": ["Consideration: {", "\t$0", "Consideration: }"]
    },
    "#define": {
        "prefix": "#def",
        "body": [
            "#define ${1/(\\S+)/${1:/capitalize}/g} =${2/^\\s+|\\s+$|^(?!\\s)|(?<!\\s)$/ /g}->${3/^\\s+|^(?!\\s)/ /g}",
            "$0"
        ]
    },
    "#define (no args)": {
        "prefix": "#defna",
        "body": ["#define ${1/(\\S+)/${1:/capitalize}/g}", "$0"]
    }
}
